{
    "builds": [{
        "src": "musify/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config":{
            "disDir": "staticfiles_build"
        }
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "musify/wsgi.py"
        }
    ]
}